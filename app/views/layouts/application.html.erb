					<%= link_to "Autos", root_path %>
				</li>
				<li class="col-md">
					<%= link_to "Piezas", "/partes/index" %>
				</li>
			</ul>
		</nav>
	</header>
<!-- Muestra los mensajes redirigidos por otra pÃ¡gina si los hay -->	
<% flash.each do |key, value| %>
   <p> <%= value %> </p>
<% end %>
<%= yield %>

</body>
</html>
